#include <stdio.h>

int main() {
    int v1, v2, v3, aux;

    printf("Ponga el valor de v1: ");
    scanf("%d", &v1);
    printf("Ponga el valor de v2: ");
    scanf("%d", &v2);
    printf("Ponga el valor de v3: ");
    scanf("%d", &v3);

    aux = v2;
    v2 = v1;
    v1 = v3;
    v3 = aux; 

    printf("\nAhora, el valor de v1 es: %d", v1);
    printf("\nAhora, el valor de v2 es: %d", v2);
    printf("\nAhora, el valor de v3 es: %d", v3);

    return 0;
}
